{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length":{
            "options":[
                120
            ]
        },
        "new-parens":true,
        "no-arg": true,
        "no-bitwise": true,
        "no-implicit-dependencies": true,
        "no-consecutive-blank-lines": false,
        "quotemark":[
            true,
            "single"
        ],
        "no-console":[
            true,
            "log"
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ]
    }
}